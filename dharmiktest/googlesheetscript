 function doPost(e) {
          var sheet = SpreadsheetApp.getActiveSpreadsheet().getActiveSheet();
          var data = JSON.parse(e.postData.contents);
    
           sheet.appendRow([
             data.timestamp,
             data.name,
             data.semester,
             data.enrollment,
            data.branch
          ]);
   
          return ContentService.createTextOutput(JSON.stringify({ "result": "success", "row": sheet.
      getLastRow() }))
            .setMimeType(ContentService.MimeType.JSON);
        }
